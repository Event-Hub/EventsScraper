<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:pro="http://www.liquibase.org/xml/ns/pro"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

  <changeSet id="0002-02" author="pdob">
    <comment>Load test data for users</comment>
    <loadUpdateData encoding="UTF-8"
                    file="database/test/testFilters.csv"
                    separator="," primaryKey="filter_id" tableName="FILTER">

      <column name="filter_id" type="BIGINT"/>
      <column name="city_id" type="BIGINT"/>
      <column name="name" type="VARCHAR(255)"/>
      <column name="from_hour" type="TIMESTAMP WITH TIME ZONE"/>
      <column name="to_hour" type="TIMESTAMP WITH TIME ZONE"/>
      <column name="user_id" type="BIGINT"/>
    </loadUpdateData>

      <sql  dbms="h2,postgresql" splitStatements="false">
        alter sequence filter_id_seq restart with 101;
      </sql>

  </changeSet>
</databaseChangeLog>
